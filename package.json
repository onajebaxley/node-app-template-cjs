  "description": "Starter template for web apps that use ExpressJS/Jade, AngularJS, and Browserify",
  "main": "index.js",
  "scripts": {
    "dev-setup": "bower install; ./node_modules/grunt-protractor-runner/node_modules/protractor/bin/webdriver-manager update",
    "dev": "export NODE_ENV=development;nodemon server/server -e js,jade",
    "start": "export NODE_ENV=production;pm2 start -i 0 server/server.js",
    "stop": "pm2 stop server/server.js",
    "test": "./node_modules/grunt-protractor-runner/node_modules/protractor/bin/webdriver-manager update; grunt"
  },
